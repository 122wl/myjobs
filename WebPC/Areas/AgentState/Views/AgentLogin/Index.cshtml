        <div class="container">
            <div class="containerBox">
                <!--登陆-->
                <div class="loginBox">
                    <div class="content clearfix">
                        <div class="left">
                            <div class='leftMain'></div>
                        </div>
                        <div class="right">
                            <h2>云客来代理商管理系统</h2>
                            <form action="/AgentLogin/Login" method="post" data-intercept="false" data-ajax="true" data-callback="CommentSuccess">
                                <p>
                                    <label>用户名：</label>
                                    @Html.TextBoxFor(x => x.UserName, new { @Class = "username" })
                                </p>
                                <p>
                                    <label>密&nbsp;&nbsp;码：</label>
                                    @Html.PasswordFor(x => x.PassWord, new { @Value = Model.PassWord, @Class = "password" })
                                </p>
                                <div class="">
                                    <a class="col-sm-6 enroll">首次登陆</a>
                                    <a class="col-sm-6 enroll2">忘记密码？</a>
                                </div>
                                <div class="btn">
                                    <button>登&nbsp;&nbsp;录</button><input type="checkbox" @(Model.PassWord == "********" ? "checked=\"checked\"" : "") value="true" name="isAuto" /> <label for="checkName">记住用户名密码</label>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
                
                
                

                <!--首次登陆-->
                <div class="validateBox">
                    <div class="login">
                        <form method="post">
                            <ul>
                                <li class="row1">
                                    <input type="text" class="name" id="phoneNum" placeholder="请输入手机号" />
                                    <!--<span onclick="sendmsg()">获取验证码</span>-->
                                    <input class="idCode" type="button" id="btn" name="btn" value="获取验证码" onclick="sendmsg()" />
                                </li>
                                <li class="row2">
                                    <input type="text" class="" placeholder="请输入" />
                                    <img src="images/yanzheng.jpg" />
                                    <a>看不清，换一张</a>
                                </li>
                                <li class="row3">
                                    <input type="text" name="" id="" value="" placeholder="请输入短信验证码" />
                                </li>
                                <li class="user-row">
                                    <input type="text" name="" id="" value="" placeholder="用户名" />
                                </li>
                                <li class="row4">
                                    <input type="password" placeholder="密码" />
                                    <div class="tips">
                                        <div class="tip">
                                            6-16字符（字母、下划线、数字）
                                            <em>&#9670;</em>
                                            <span>&#9670;</span>
                                        </div>
                                    </div>
                                </li>
                                <li class="row5">
                                    <input type="password" placeholder="请再输入一次" />
                                </li>
                                <li class="row6">
                                    <input type="submit" class="submit" value="确定" />
                                </li>
                            </ul>
                        </form>
                    </div>
                </div>

                <footer>
                    <p><span>Copyright©2015</span>杭州慧格网络科技有限公司</p>
                </footer>
